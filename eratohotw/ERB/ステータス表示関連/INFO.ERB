	IF CFLAG:LOCAL:現在位置 == CFLAG:MASTER:現在位置
		;まだ同室フラグが立っていない(現ターンで同室化）かつ眠っていない
		SIF !CFLAG:LOCAL:同室 && !CFLAG:LOCAL:睡眠
			CALL AFFAIR_DISCLOSURE(LOCAL, 0)
		;情事の発覚 AFFAIR_DISCLOSURE で立ち去った場合は同室が立たない
		SIF !RESULT
			CFLAG:LOCAL:同室 = 1
	ELSE
