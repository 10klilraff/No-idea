;-------------------------------------------------
;古典時刻関数@CLASSICAL_TIME
;	引数0：対象の変数（デフォでは秒）
;	引数1：1なら引数0を秒で、2なら時で計算する
;	引数2：表示形式（デフォ「○の○つ」、1=「○の刻、○つ」、2=「○の○つ　-○○-」）
;式中で使用する関数です。引数に記録されている数字を秒として、現在の時刻を文字列で返します
;古典時刻表記なので、30分単位未満の端数は切り捨てます。
;-------------------------------------------------
@CLASSICAL_TIME(ARG, ARG:1, ARG:2)
#FUNCTIONS
;0:00が子の三つなので、60分を足す
ARG += 60
;日付も管理しているケースを考え、24時間（1440分）で割ってあまりを取る（子の一つ～二つもここで対応される）
ARG %= 1440
;表示
SIF GET_CLASSICAL_HOUR(ARG, 1) == "Ox" && (ARG%120)/30 == 2
	RETURNF "Witching Hour"
;custom code, reordered
RETURNF @"%TOKANJI(((ARG%120)/30)+1)% Of The %GET_CLASSICAL_HOUR(ARG, 1)%\@ ARG:2 == 1 ? 刻、 # \@ \@ ARG:2 == 2 ?  -%GET_CLASSICAL_HOUR(ARG, 1, 1)%- # \@"

;時を取得
;引数2を真にすると、大まかな時間の様子を取る
@GET_CLASSICAL_HOUR(ARG, ARG:1, ARG:2)
#FUNCTIONS
;分に変換する
SELECTCASE ARG:1
	;時で渡す場合
	CASE 2
		ARG *= 60
	;分で渡す場合（処理なし）
	CASE 1
	;秒で渡す場合（デフォルト）
	CASEELSE
		ARG /= 60
ENDSELECT

SELECTCASE ARG/120
	CASE 0
		RETURNF \@ ARG:2 ? Midnight # Rat \@
	CASE 1
		RETURNF \@ ARG:2 ? Early dawn # Ox \@
	CASE 2
		RETURNF \@ ARG:2 ? Dawn # Tiger \@
	CASE 3
		RETURNF \@ ARG:2 ? Sunrise # Rabbit \@
	CASE 4
		RETURNF \@ ARG:2 ? Early morning # Dragon \@
	CASE 5
		RETURNF \@ ARG:2 ? Morning # Snake \@
	CASE 6
		RETURNF \@ ARG:2 ? Noon # Horse \@
	CASE 7
		RETURNF \@ ARG:2 ? High noon # Goat \@
	CASE 8
		RETURNF \@ ARG:2 ? Afternoon # Monkey \@
	CASE 9
		RETURNF \@ ARG:2 ? Sunset # Rooster \@
	CASE 10
		RETURNF \@ ARG:2 ? Evening # Dog \@
	CASE 11
		RETURNF \@ ARG:2 ? Late evening # Pig \@
ENDSELECT

;-------------------------------------------------
;数値→漢数字変換関数@TOKANJI
;TOSTR関数チックに、引数の数値を漢数字の文字列で返します
;変数の最大値は約922京であるため、一、万、億、兆、京の5回処理します
;-------------------------------------------------
@TOKANJI(ARG)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM LCOUNT, 1
VARSET LOCALS
FOR LCOUNT, 0, 5
	LOCAL = ARG/POWER(10000, (4-LCOUNT))
	ARG %= POWER(10000, (4-LCOUNT))
	IF LOCAL
		LOCALS += TOKANJIT(LOCAL)
		SELECTCASE LCOUNT
			CASE 0
				LOCALS += "京"
			CASE 1
				LOCALS += "兆"
			CASE 2
				LOCALS += "億"
			CASE 3
				LOCALS += "万"
		ENDSELECT
	ENDIF
NEXT
RETURNF \@ LOCALS != "" ? %LOCALS% # 零 \@

;内部用or簡易用
;四桁以内の漢字変換。万以上与えるとバグ
@TOKANJIT(ARG)
#FUNCTIONS
SIF ARG >= 10000
	RETURNF "Bug"
LOCALS = \@ ARG/1000 ? %TOKANJIS(ARG/1000, 1)%千 # \@
ARG %= 1000
LOCALS += \@ ARG/100 ? %TOKANJIS(ARG/100, 1)%百 # \@
ARG %= 100
LOCALS += \@ ARG/10 ? %TOKANJIS(ARG/10, 1)%十 # \@
ARG %= 10
RETURNF LOCALS + TOKANJIS(ARG)

;一桁の変換。引数1を真にすると一を省略する。2以上にすると零を表示する
@TOKANJIS(ARG, ARG:1)
#FUNCTIONS
SELECTCASE ARG
	CASE 0
		RETURNF \@ ARG:1 > 1 ? 零 #  \@
	CASE 1
		RETURNF \@ ARG:1 ?  # First \@
	CASE 2
		RETURNF "Second"
	CASE 3
		RETURNF "Third"
	CASE 4
		RETURNF "Fourth"
	CASE 5
		RETURNF "Fifth"
	CASE 6
		RETURNF "Sixth"
	CASE 7
		RETURNF "Seventh"
	CASE 8
		RETURNF "Eighth"
	CASE 9
		RETURNF "Ninth"
	CASE 10
		RETURNF "Tenth"
	CASEELSE
		RETURNF "Bug"
ENDSELECT



