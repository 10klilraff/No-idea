@M_KOJO_MESSAGE_COM_K105_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K105_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TCVAR:泥酔 == 1
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「そーだね、水分……」
			PRINTDATAW
				DATAFORM 「ふぅ……ひと安心%UNICODE(0x2665) *1%」
				DATAFORM 「……あっつ！　あつい！！」
			ENDDATA
		ELSE
			PRINTFORML 「ああ、ありがと」
			PRINTFORMW 「はぁー……なにこれ、お茶？」
		ENDIF
	ELSEIF TFLAG:193 == 1
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「...Ah, it's really delicious this time...%UNICODE(0x2665) *1%」
			PRINTFORMW 「Please make it like this every time.」
		ELSE
			PRINTFORML 「...Ah, delicious.」
			PRINTFORMDW Mai is staring longingly at her tea...
		ENDIF
	ELSEIF TFLAG:193 == 0
		IF CFLAG:318 == 1
			PRINTFORML 「......」
			PRINTFORMDW Mai is silently drinking her tea...
		ELSEIF TALENT:恋人
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「%CALLNAME:MASTER%, tea!」
				PRINTFORMW 「...Eh, that was so fast――Hothot!」
				PRINTFORMW 「What's going on here... and it's delicious to boot.」
			CASE 1
				PRINTFORML 「Ahー, thank you.」
				PRINTFORMW 「Nn...」
				PRINTFORMW 「How relaxing...」
			CASE 2
				PRINTFORML 「It's your fault that I've become so addicted to tea, %CALLNAME:MASTER%.」
				PRINTFORMW 「Maybe I've even gotten more picky about my preferences.」
				PRINTFORMW 「Do your best.」
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「Aah, somehow my throat's feeling――」
				PRINTFORMW 「――Ah, thank you... What coincidental timing.」
				;Japs deleted following line
				;PRINTFORMW 「There's no way that you were able to immediately brew tea after hearing that... right?」
			CASE 1
				PRINTFORML 「Thank you again.」
				PRINTFORMW 「Fuuー...」
				PRINTFORMW 「Fufu, delicious...%UNICODE(0x2665) *1%」
			CASE 2
				PRINTFORML 「Hmm, this one is different from the last time.」
				PRINTFORMW 「What's type is this one?」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「...I'll accept it.」
				PRINTFORMW 「Nn...」
			CASE 1
				PRINTFORMW 「...Hot.」
				PRINTFORMW 「I'm fine.」
			CASE 2
				PRINTFORML 「...」
				PRINTFORMW （Is it %HIS_HER(MASTER)% hobby to offer tea to anyone he meets?）
			ENDSELECT
		ELSE
			PRINTFORML 「...Nngh...」
			PRINTFORMDW Mai is silently drinking her tea.
		ENDIF
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「...Ueh, terrible.」
			PRINTFORMW 「Do you know how much I like you serving me tea, %CALLNAME:MASTER%? This is how much.」
		ELSE
			PRINTFORML 「...」
			PRINTFORMDW Mai doesn't drink her tea...
		ENDIF
	ENDIF
ENDIF
RETURN 1
