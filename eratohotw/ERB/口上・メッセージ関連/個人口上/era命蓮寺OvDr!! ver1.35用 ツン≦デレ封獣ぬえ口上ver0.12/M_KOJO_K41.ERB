	;空白は受け付けない
	ELSEIF RESULTS == ""
		PRINTW Input a name.
		CONTINUE
	ELSE
		; Same name
		; we skip some name matches because either they're so short or not so unique, so we check them by their surname instead which should be a right pointer
		IF FLAG:なりきり != 41 && STRCOUNT(LOCALS, "^(?i)nue$")
			PRINTFORMW 「Liar. You sure are unidentifiable, my ass.」
			CONTINUE
		; 白蓮が主人ではなく、白蓮というフレーズが含まれている。もしくは
		; 星が主人ではなく、寅丸もしくは星というフレーズが含まれている
		ELSEIF (FLAG:なりきり != 55 && STRCOUNT(LOCALS, "(?i)byakuren|hijiri")) || (FLAG:なりきり != 87 && STRCOUNT(LOCALS, "(?i)toramaru|shou"))
			PRINTFORMW 「Yeah yeah, quit fooling around.」
			CONTINUE
		; 命蓮寺勢はじき
		; うんそうだね多々良勝五郎とか教える人もいるかもしれないもんね
		ELSEIF (FLAG:なりきり != 39 && STRCOUNT(LOCALS, "(?i)nazrin")) || (FLAG:なりきり != 40 && STRCOUNT(LOCALS, "(?i)tatara|kogasa")) || (FLAG:なりきり != 85 && STRCOUNT(LOCALS, "(?i)kumoi|(?i)ichirin")) || (FLAG:なりきり != 86 && STRCOUNT(LOCALS, "(?i)minamitsu|murasa"))
			PRINTFORMW 「There's already a person with this name. Can't you give me a nickname instead?」
			CONTINUE
		ELSEIF FLAG:なりきり != 69 && STRCOUNT(LOCALS, "(?i)futatsuiwa|mamizou")
			PRINTFORMW 「Mamizou, huh? Since when did you become so young?」
			CONTINUE
		; 道教勢力はじき
		;we use a more elaborate regex for yoshika because that name is not as unique compared to others, so only match if they're together
		ELSEIF (FLAG:なりきり != 89 && STRCOUNT(LOCALS, "^(?=.*\b[M|m]iyako\b)(?=.*\b[Y|y]oshika\b).*$")) || (FLAG:なりきり != 90 && STRCOUNT(LOCALS, "(?i)kaku|seiga")) || (FLAG:なりきり != 91 && STRCOUNT(LOCALS, "(?i)soga|(?i)tojiko")) || (FLAG:なりきり != 92 && STRCOUNT(LOCALS, "(?i)mononobe")) || (FLAG:なりきり != 56 && STRCOUNT(LOCALS, "(?i)toyosatomimi|shoutoku|taishi"))
			PRINTFORMW 「Geh, can't you come up with something better?」
			CONTINUE
		; 博麗霊夢というフレーズが含まれてる。もしくは
		; 東風谷早苗というフレーズが含まれている
		ELSEIF STRCOUNT(LOCALS, "(?i)hakurei") || STRCOUNT(LOCALS, "(?i)kochiya")
			PRINTFORML 「Huh, are you that shrine maiden's relative or something? Why are you even here?」
			PRINTFORML 「...Well, whatever. None of my business, I guess.」
			PRINTFORML
		; 霧雨魔理沙というフレーズが含まれてる。
		ELSEIF STRCOUNT(LOCALS, "(?i)kirisame")
			PRINTFORML 「Huh, are you that magician's relative or something? Why are you even associating with Buddhists?」
			PRINTFORML 「...Well, whatever. None of my business, I guess.」
			PRINTFORML
		; 直接入力ならいけると思ったか？弾くぜ！
		;just checking FIRSTTIME("U41-1") invalidates it for repeated checks, so removed
		;any kinds of niichan/niisan will be matched (so onisan, oniichan, etc)
		ELSEIF ABL:親密 < 4 && STRCOUNT(LOCALS, "((([N|n]i{1,})|([N|n]e{1,}))(-|)(san|chan)|[B|b]rother|[S|s]ister|[B|b]ro|[S|s]is)$")
			PRINTFORMW 「Can't you take it a little more seriously?」
			CONTINUE
		ELSEIF ABL:親密 < 7 && STRCOUNT(LOCALS, "(([N|n]i{1,})|([N|n]e{1,}))(-|)(chama|sama)$")
			PRINTFORMW 「Can't you take it a little more seriously?」
			CONTINUE
		ELSEIF (ABL:従順 < 7 || ABL:奉仕精神 < 7) && STRCOUNT(LOCALS, "((?i)master|lord|god)$")
			PRINTFORMW 「Can't you take it a little more seriously?」
			CONTINUE
		ELSEIF (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "(?i)(husband|dear|darling|honey|hubby)$")
			PRINTFORMW 「Can't you take it a little more seriously?」
			CONTINUE
		ENDIF
		
		PRINTFORML 「So "%RESULTS%", right?」
		CALL ASK_YN
		IF RESULT == 0
			;あなたの名前を%MASTERNAME:41%に格納
			MASTERNAME:41 = %RESULTS%
			PRINTFORMDL 
			BREAK
		ELSEIF RESULT == 1
			CONTINUE
		ENDIF
	ENDIF
LOOP 1
