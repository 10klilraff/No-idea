;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_ENG01_MESSAGE_COM_K15_301
CALL TRAIN_MESSAGE
CALL M_KOJO_ENG01_MESSAGE_COM_K15_301_1
RETURN RESULT

@M_KOJO_ENG01_MESSAGE_COM_K15_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「You made some tea for me?」
		PRINTFORML 「I was just about to make some myself, but it would be rude to refuse.」
		PRINTFORMDW She takes a sip from your tea
		PRINTFORMW 「It doesn't taste too bad.」
		
		IF K15_PLAYER_IS_SDM_RESIDENT()
			PRINTFORML 「As a servant of the mansion, you will need to learn how to make tea the correct way.」
			PRINTFORMW 「Of course, I will try to teach you.」
		ENDIF
		
		RETURN 1
	ENDIF
	;-------------------------------------------------
	IF K15_HATE_ANGER()
		PRINTFORMW 「Very well, I'll try it.」
		PRINTFORMDL She reluctantly accepts the tea you made for her.
	
		IF TFLAG:193 == 1
			PRINTFORMDL %CALLNAME:15% ignores you while enjoying the tea break, you can see that see enjoys it.
			PRINTFORMW 「It didn't taste too bad.」
		ELSEIF TFLAG:193 == -1
			PRINTFORMDL %CALLNAME:15% ignores you while taking a single sip.
			PRINTFORMW 「It wasn't to my taste.」
		ELSE
			PRINTFORMDL %CALLNAME:15% ignores you while enjoying the tea break.
		ENDIF
	;-------------------------------------------------
	ELSE
		IF TFLAG:193 == 1
			PRINTFORML 「Thank you. Let's enjoy a small break.」
			PRINTFORMDL You see her smile softly as she enjoys the tea you made.
			PRINTFORMW 「It tastes good, thank you again.」
		ELSEIF TFLAG:193 == -1
			PRINTFORMDL You see her taking a sip and looking a bit disappointed.
			PRINTFORMW 「I'm sorry, it wasn't to my tastes.」
		ELSE
			PRINTFORML 「Thank you. Let's enjoy a small break.」
			PRINTFORMDW %CALLNAME:15% enjoys the tea break.
		ENDIF
	ENDIF
	
	CALL K15_SAKUYA_SPECIAL_EVENTS()
ENDIF
RETURN 1
