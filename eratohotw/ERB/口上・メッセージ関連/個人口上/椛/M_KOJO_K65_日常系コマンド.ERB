						PRINTFORML 「%CALLNAME:MASTER%-san... Please make me feel better and better inside my belly...」
					ELSE
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「Fuu～...Mn, AhnHPH」","L"
								PRINTFORML 「%CALLNAME:MASTER%-san, my belly...Mmmn... Does it feel good...?」
								PRINTFORMDW Momiji works her hips on your shaft while gazing into your eyes...
							CASE 1
								PRINTFORML 「Don't worry, just keep going, let me handle it...」
								PRINTFORMW 「Move a lot, and I'll make you feel good, %CALLNAME:MASTER%-san...」
						ENDSELECT
					ENDIF
				ENDIF
			;86=だいしゅきホールド
			CASE 86
				IF TCVAR:発情
					CALL HPH_PRINT,@"「Come for me...HPH Please, you have to finish all the way in my deepest, tightest place...HPH」","W"
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%-san... Do it right there, in my belly...!」
				ENDIF
			CASEELSE
				IF TCVAR:発情
					IF BASE:MASTER:気力 <= 0
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「Aha, You're so tired you can't move...?」
								CALL HPH_PRINT,@"「Ehehe... Then I'll attack the silly %CALLNAME:MASTER%-san who can't move...HPH」","W"
							CASE 1
								PRINTFORML 「Aha, You must have gotten tired...」
								CALL HPH_PRINT,@"「Don't worry, I'll violate you plenty, %CALLNAME:MASTER%-san...HPH」","W"
						ENDSELECT
					ELSE
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%-san, more...HPH」","W"
					ENDIF
				ELSE
					IF BASE:MASTER:気力 <= 0
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「Aha, You're so tired you can't move...?」
								PRINTFORMW 「Fufu, It's fine. I'll take care of it...」
							CASE 1
								PRINTFORML 「If you're tired, you don't have to move...」
								PRINTFORMW 「I'll make you feel good, %CALLNAME:MASTER%-san...」
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「%CALLNAME:MASTER%-san, does it feel good...?」
							CASE 1
								PRINTFORMW 「%CALLNAME:MASTER%-san... Don't move so much...」
						ENDSELECT
					ENDIF
				ENDIF
		ENDSELECT
	;恋慕、思慕などで口上を変化させたい場合は↓のコメントアウトを外してください。
	;ENDIF
ENDIF
