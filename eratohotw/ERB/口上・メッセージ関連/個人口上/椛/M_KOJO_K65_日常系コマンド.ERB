;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K65_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ > 0
		PRINTFORML 「...Thanks.」
		PRINTFORMDW Momiji sits in silence as she sips the tea you made.
	;仕事中
	ELSEIF CFLAG:行動 == 5
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 「Thank you very much. This is pretty thirsty work.」
			CASE 1
			PRINTFORML 「Ah, thank you very much.」
			PRINTFORMW 「I presume you want me to take a break then?」
		ENDSELECT
	ELSE
		;大成功
		IF TFLAG:193 == 1
			SELECTCASE RAND:2
				CASE 0
				PRINTFORML 「It smells nice...」
				PRINTFORML 「Just smelling it makes me feel good.」
				PRINTFORMDW Momiji closes her eyes as if entranced, enjoying the fragrant aroma of the tea.
				CASE 1
				PRINTFORML 「Tea? Thank you very much.」
				PRINTFORML 「...It's extremely delicious.」
				PRINTFORMW 「Is there some kind of trick to how you do it?」
			ENDSELECT
		;失敗
		ELSEIF TFLAG:193 == -1
		PRINTFORML 「Tea? Thank you very much.」
		PRINTFORML 「... -Ghu!? Cough, cough...」
		PRINTFORMDW Momiji tries to drink it, but ends up choking on it instead...
		;成功
		ELSE
			SELECTCASE RAND:2
				CASE 0
				PRINTFORMW 「Tea? Thank you very much.」
				CASE 1
				PRINTFORML 「Ah, thank you, itadakimasu...」
				PRINTFORMDW Momiji receives the cup respectfully, and begins sipping on the tea.
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
