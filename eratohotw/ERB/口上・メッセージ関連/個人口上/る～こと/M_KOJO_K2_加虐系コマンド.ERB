						CALL K2_HPH_PRINT,@"「Forgive your second-hand robot for allowing other penises inside her HPH」","W"
					ELSEIF ABL:清掃技能 + ABL:料理技能 < 4
						CALL K2_HPH_PRINT,@"「Please punish your shameless maid not knowing about anything other than having sex. Remind her what her job is HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「Nh, Haanh HPH Forgive this slutty maid for sleeping with others HPH」","W"
					ENDIF
				ELSE
					GOTO SPANKING_LOOP
				ENDIF
			CASE 2
				CALL K2_HPH_PRINT,@"「Ahaa HPH Please punish this useless maid even more HPH」","W"
			CASE 3
				CALL K2_HPH_PRINT,@"「Nfuu HPH Haa HPH It hurts, and yet it feels wonderful HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「Ahhii HPH Khaa, Nh HPH Nhaa, Hah... Huh... Is it over? HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「Uuu... Being spanked hurts...」
			CASEELSE
				PRINTFORMW 「Nku... My butt feels numb...」
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「Khu... It hurts, please stop.」
		CASEELSE
			PRINTFORMW 「Oww, Nh, Haa, Haah... S-stop it, please...」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

