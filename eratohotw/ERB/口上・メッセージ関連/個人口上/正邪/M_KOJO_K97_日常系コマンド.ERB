;==================================================
;626,土産屋
;added, based on hatate
;TFLAG:194(行動結果(-2=ひやかし -1=ウィンドウショッピング 1=400点未満（BAD） 2=～699点（NOMAL） 3=700点以上（GOOD）
;CFLAG:性格傾向による地の文分岐あり
;==================================================
;映姫様口上作者様から拝借しました。いつもすみません
;==================================================
@M_KOJO_MESSAGE_COM_K97_626
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K97_626_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_626_1
#DIMS strGift
#DIMS strReaction
#DIM nPrice
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;贈っていたら本体名と属性を取得しておく
	IF TFLAG:194 > 0
		CALL GIFTDATA_MAIN(GET_GIFTDATA(TCVAR:97:今日の贈り物, "本体"))
		strGift = %RESULTS%
		nPrice = RESULT:1
		IF STRFIND(RESULTS:1, "抱く") >= 0
			strReaction = 抱く
		ELSEIF STRFIND(RESULTS:1, "眺める") >= 0
			strReaction = 眺める
		ELSEIF STRFIND(RESULTS:1, "握る") >= 0
			SIF STRFIND(RESULTS:1, "装飾") >= 0
				strReaction = 身につける
			strReaction = 握る
		ELSE
			strReaction = 身につける
		ENDIF
	ENDIF
	
	SIF FLAG:口上色
		TRYCALLFORM M_KOJO_COLOR_K{NO:TARGET}
	SELECTCASE TFLAG:194
	;ひやかし
	CASE -2
		IF TALENT:恋慕
			IF RAND:2
				PRINTFORML 「Well, if you didn't find what you want, then let's get going.」
			ELSE
				PRINTFORML 「Whatever. Let's not waste time here.」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORML 「You don't need to force yourself to buy me anything, you know.」
			ELSE
				PRINTFORML 「That's for the best. I don't feel like having some useless trinket anyway.」
			ENDIF
		ENDIF
	;やっぱりやめる（見送り）
	CASE -1
		IF RAND:2
			PRINTFORML 「What, you wanted to grab something for me?」
			PRINTFORML 「Well, I've got some specific tastes. Finding what I like would be difficult.」
		ELSE
			PRINTFORML 「Heh, did you see that clerk? She was totally fuming.」
			PRINTFORML 「Well, too bad.」
		ENDIF
	;400点未満（BAD）
	CASE 1
		PRINTFORML 「Huh, is that for me?」
		PRINTFORML 「...Hate to break it to you, but I'll probably toss it away.」
		SIF nPrice >= 30000
			PRINTFORML 「Or maybe I'll just pawn it. Yeah, thanks for that at least.」
	;400点～699点（NOMAL）
	CASE 2
		IF TALENT:恋慕 || TALENT:恋人
			PRINTFORML 「Wow, you're giving this to me?」
			PRINTFORML 「...Thanks, I'll treasure it.」
			PRINTFORML 「You chose what I actually like, good on you. I knew you can do it.」
		ELSE
			PRINTFORML 「What, is that for me? Really?」
			PRINTFORML 「Hmm... Yeah, I think I'll... Keep it.」
			IF nPrice >= 30000
				PRINTFORML 「But y'know, you shouldn't splurge so much money on this stuff.」
				PRINTFORML 「You're not impressing anyone.」
			ENDIF
		ENDIF
	;700点以上（GOOD）
	CASE 3
		;形容詞+本体名を取得
		CALL GIFTNAME(TCVAR:97:今日の贈り物)
		PRINTFORML 「Hey, is that %RESULTS%... It's for me? You sure?」
		PRINTFORML 「Thanks, %CALLNAME:MASTER%. I really mean it.」
		PRINTW
		SELECTCASE strReaction
		CASE "眺める"
			PRINTFORML 「It looks something like I'd just admire forever.」
			PRINTFORML 「I'll treasure it for sure.」
		CASE "抱く"
			PRINTFORML 「Wow, I think... I'm in love with this thing.」
			PRINTFORML 「I never thought I'd need this in my life.」
		CASE "握る"
			PRINTFORML 「Aah, it looks even better when I hold it in my hands.」
			PRINTFORML 「I really wanted this one at some point. Yeah, I'll be really careful with it.」
		CASE "身につける"
			PRINTFORML 「Hm, is this something I gotta put on? Alright... Turn away.」
			PRINTFORML 「There, how does it look? Is it good? ...Heh, thought so.」
			PRINTFORML 「Amazing stuff, I'll be sure to flaunt it wherever I go from now on.」
		ENDSELECT
		
		IF nPrice >= 30000
			PRINTW
			PRINTFORML 「...Wait, this must've been expensive as hell.」
			PRINTFORML 「Are you sure it's alright to give me something like this? I hardly deserve that...」
			PRINTFORML 「Gifts are not always good just because they cost an arm and a leg.」
		ENDIF
		
		IF TALENT:恋人
			PRINTFORML 「I know this isn't much, but... Smoooch!%UNICODE(0x2665) *1%」
			PRINTFORML 「Just a little token of appreciation from me. On the cheek, I know, but we'll do more... later.」
		ENDIF
	ENDSELECT
	PRINTW
ENDIF
RETURN 1

