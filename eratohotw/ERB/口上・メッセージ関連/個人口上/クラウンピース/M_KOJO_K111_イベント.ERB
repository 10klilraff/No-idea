;■専用関数
;不機嫌判定
@M_KOJO_K111_FUKIGEN(ARG)
#FUNCTION
SIF TALENT:ARG:機嫌 == -1
	RETURNF 1
SIF CFLAG:ARG:ブチギレ
	RETURNF 1
SIF MARK:ARG:反発刻印 >= 3
	RETURNF 1
RETURNF 0

;おうち判定
;1=クラウンピースの部屋,2=純狐邸,3=入り江
@M_KOJO_K111_HOUSE(ARG)
#FUNCTION
IF MAIN_MAP == 10
	IF ARG == CFLAG:[[クラウンピース]]:初期位置
		RETURNF 1
	ELSEIF ARG == 1016
		RETURNF 3
	ELSEIF INRANGE(ARG, 1060, 1072)
		RETURNF 2
	ENDIF
ELSE
	SIF ARG == OMANEKIBEYA() || ARG == CFLAG:[[クラウンピース]]:初期位置
		RETURNF 1
ENDIF

;純狐邸に住んでる判定,引数なし
@M_KOJO_K111_JUNKO_KURASHI
#FUNCTION
;月じゃないなら×
SIF MAIN_MAP != 10
	RETURNF 0
;一応二段階で設定しておく
SIF INRANGE(CFLAG:MASTER:初期位置, 1060, 1072)
	RETURNF 1
SIF GROUPMATCH(CFLAG:MASTER:初期位置, CFLAG:[[クラウンピース]]:初期位置, CFLAG:[[純狐]]:初期位置, CFLAG:[[ヘカーティア]]:初期位置)
	RETURNF 1

;地上判定
@M_KOJO_K111_EARTH(ARG)
#FUNCTION
;神社～森
IF ARG < 600
	RETURNF 1
;冥界
ELSEIF INRANGE(ARG, 600, 700)
	IF MAIN_MAP == 6
		;中有の道～積石塚
		IF INRANGE(ARG, 601, 603)
			RETURNF 1
		;八雲邸
		ELSEIF INRANGE(ARG, 630, 639)
			RETURNF 1
		ENDIF
	ELSE
		;中有の道、マヨヒガ
		SIF GROUPMATCH(ARG, 610, 630)
			RETURNF 1
	ENDIF
;山麓
ELSEIF INRANGE(ARG, 700, 800)
	RETURNF 1
;山頂
ELSEIF INRANGE(ARG, 800, 900)
	IF MAIN_MAP == 8
		;入り口～守矢神社
		SIF INRANGE(ARG, 801, 825)
			RETURNF 1
	ELSE
		;九天の滝～守矢神社本殿
		SIF INRANGE(ARG, 810, 850)
			RETURNF 1
	ENDIF
;地底～月
ELSEIF INRANGE(ARG, 900, 1100)
	RETURNF 0
ENDIF

;あなたの印象
@M_KOJO_K111_MASTER_IMPRESSION
#FUNCTIONS
VARSET LOCALS
VARSET LOCAL
SIF CFLAG:[[クラウンピース]]:既成事実
	LOCALS:1 = ロリコン
SIF TALENT:MASTER:清掃中毒 || ABL:MASTER:清掃技能 >= 6
	LOCALS:2 = 清掃中毒者
SIF LEARNED_MENU:[[クラウンピース]] >= 100
	LOCALS:3 = 料理人
SIF CFLAG:[[クラウンピース]]:弾幕勝負勝利
	LOCALS:4 = 弾幕強者
SIF CFLAG:[[クラウンピース]]:ムシバトル直接勝利
	LOCALS:5 = 昆虫王
SIF TALENT:MASTER:恋人 && !GROUPMATCH(TALENT:MASTER:恋人, [[クラウンピース]], [[ヘカーティア]], [[純狐]])
	LOCALS:6 = モテ男
SIF ABL:MASTER:教養 >= 4
	LOCALS:7 = インテリ
SIF ABL:MASTER:話術技能 >= 5
	LOCALS:8 = 饒舌
SIF MONEY >= 1000000
	LOCALS:9 = 金持ち
FOR LOCAL, 1, 10
	IF LOCALS:LOCAL != ""
		LOCAL:1 = 1
		BREAK
	ENDIF
NEXT
;何も特徴が無い
SIF !LOCAL:1
	LOCALS:10 = 平凡

LOCAL = 0
WHILE LOCALS == ""
	LOCAL = RAND:10 + 1
	SIF LOCALS:LOCAL != ""
		LOCALS = %LOCALS:LOCAL%
WEND
RETURNF LOCALS

