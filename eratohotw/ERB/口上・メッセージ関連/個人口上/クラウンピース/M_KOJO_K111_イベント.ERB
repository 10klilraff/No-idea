;==================================================
;相手からデートのお誘い
;==================================================
@M_KOJO_EVENT_K111_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「Hey hey, %CALLNAME:MASTER%, let's go out♪」
			SELECTCASE STR:(5000 + ARG:1)
			CASE "博麗神社"
				PRINTFORMW 「I want to go to the Hakurei Shrine～♪」
			CASE "命蓮寺"
				PRINTFORMW 「I want to go to the Myouren Temple～♪」
			CASE "人間の里"
				PRINTFORMW 「Let's go on a date in the village! Date～♪」
			CASE "霧の湖～紅魔館"
				PRINTFORMW 「I want to go to the Misty Lake～♪」
			CASE "迷いの竹林"
				PRINTFORMW 「I want to go to the Bamboo Forest of the Lost～♪」
			CASE "魔法の森"
				PRINTFORMW 「I want to go to the Forest of Magic～♪」
			CASE "三途の川～冥界"
				PRINTFORML 「Sanzu River, let's cross with together!」
				PRINTFORMW 「It'll be fine～ %CALLNAME:MASTER% and I can go to hell together～」
			CASE "妖怪の山 (山麓)"
				PRINTFORMW 「I want to go to the Youkai Mountain～♪」
			CASE "妖怪の山 (山頂)"
				PRINTFORMW 「I want to try mountain climbing～♪」
			CASE "地底"
				PRINTFORMW 「I want to go to the Netherworld～♪」
			CASE "夢の世界～月"
				PRINTFORMW 「Let's go to the Moon♪ Do you think it is far? It's close from here!」
			ENDSELECT
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「Yay♪ %CALLNAME:MASTER% is so kind♪」
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「Fuu, So mean.」
			PRINTFORMW 「Oh well♪ See you next time♪」
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「Hey hey, Why don't you come to visit me today?」
			IF FIRSTTIME("お招き", 1, 111)
				PRINTFORML 「There's no parents today!」
				PRINTFORMW 「... It's always like that.」
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「There's something I want to show to %CALLNAME:MASTER%♪」
				CASE 1
					PRINTFORMW 「I want to eat %CALLNAME:MASTER%'s cooking, just kidding.」
				CASEELSE
					PRINTFORMW 「Ah, Umm, It's fine if it's just the two of us...」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("お招き", 0, 111)
				PRINTFORML 「Really!? Yay♪」
				PRINTFORMW 「It might be a bit messy, but I'm sure %CALLNAME:MASTER% will like it♪」
			ELSE
				;晴れ
				IF TIME:5 <= 1
					PRINTFORML 「Yay!」
					PRINTFORML 「Let's hold hands! I'm going to fly away!」
					;TIME += -1 * TIME_REQUIRED(GET_MAPID(CFLAG:MASTER:現在位置), 10) / 2
					;遠いから時間短縮してあげようと思ったけど直後に出る地の文の表示と齟齬が出るのでやめ。-X分ってのは変だよね…
				ELSE
					PRINTFORML 「Oh, Thank goodness!」
					PRINTFORMW 「Good thing I decided to clean up the room♪」
					;とか言ってるけど部屋の汚れは変化しません（ここではできない）
					;本人は精一杯部屋を片付けたつもりでいるのです！
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「I see...」
			PRINTFORMW 「The moon is too far away after all...」
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K111_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方選択
;ARG:1（ARG=3の場合のみあり）地の文の反応
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:時間停止
				IF TCVAR:111:350
					PRINTFORML You remove Clownpiece's clothes, they're stuffy and hot.
					PRINTFORMW You take in the sweet, bewitching scent.
				ENDIF
			;通常
			ELSE
				IF TCVAR:111:350
					PRINTFORML You remove Clownpiece's clothes, they're stuffy and hot.
					PRINTFORMW 「Don't smell it! Gross!」
				ELSE
					PRINTFORML 「Then %CALLNAME:MASTER% take it off%UNICODE(0x2764)%」
					PRINTFORMW 「Fuuー Refreshing♪」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交換です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「Hey hey, Let's trade♪」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TCVAR:111:350
						PRINTFORML 「Eehー... Today is a bit...」
						PRINTFORMW 「You know, I got sweaty...」
						PRINTFORMW 「Wait, Why are you smiling like that!? Pervert!」
					ELSE
						PRINTFORML 「Eh? Panties?」
						PRINTFORMW 「What do I have to wear after this?」
						PRINTFORMW 「Well fine, if you want them %CALLNAME:MASTER%, I'll give you mine%UNICODE(0x2764)%」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TCVAR:111:350
						PRINTFORML 「Eehー... Today is a bit...」
						PRINTFORMW 「You know, I got sweaty...」
						PRINTFORMW 「Wait, Why are you smiling like that!? Pervert!」
					ELSE
						PRINTFORML 「Eh? Panties?」
						PRINTFORMW 「What are you going to do with that...?」
						PRINTFORMW 「Well fine, if you want them %CALLNAME:MASTER%, I'll give you mine but...」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORML 「Eh? Panties?」
					PRINTFORMW 「No way! Gross!」
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

