;621,釣りをする
;TFLAG:193(0=釣れない 1～4=魚 11～15=うまい魚 610=木材 131=守矢の守札 21=ローター 609=氷の鱗 606=龍の爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K111_621
;CALL TRAIN_MESSAGE
RESETCOLOR
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K111_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_621_1
#DIM CHARA
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;地の文をカットしてるのでMESSAGE処理を内蔵する
IF LOCAL
	CALL M_KOJO_COLOR_K111
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
PRINTFORMDW 何も釣れなかった……
		IF TALENT:111:恋慕
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「あ～～釣れない～～っ！」
			CASE 2
				PRINTFORMW 「さかなさかなさかな～♪」
			CASEELSE
				PRINTFORMW 「%CALLNAME:MASTER%、タイクツ～」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「……ひまー！！」
			CASE 2
				PRINTFORMW 「……これ、楽しい？」
			CASEELSE
				PRINTFORMW 「... Nothing is biting...」
			ENDSELECT
		ENDIF
		RETURN 1
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「Look! I caught something!!」
			PRINTL 
			PRINTFORMDL %CALLNAME:TARGET% caught a piece of wood...
			PRINTW
			PRINTFORMW 「Nnhh～～～!!」
		ELSE
			PRINTFORMDL You caught some wood......
			PRINTW 
			PRINTFORMW 「Kyahaha! Miss～!」
		ENDIF
		RETURN 1
	;紙切れ
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORML 「Look! I caught something!!」
		ELSE
			PRINTFORMDL You caught some paper...
		ENDIF
		PRINTDW 
		PRINTDL ┌─────────────┐
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　 Moriya Talisman 　 　｜
		PRINTDL ｜　　　　　Lottery 　　　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　Gather 5 Talismans  　｜
		PRINTDL ｜　　　Get a lottery roll　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　Miracle not guaranteed　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　　　　　Moriya Shrine ｜
		PRINTDL └─────────────┘
		PRINTW 
		PRINTFORMW 「...? What is this? Trash?」
		RETURN 1
	CASE 1 TO 20, 22 TO 60
		LOCALS = %FISH_NAME(TFLAG:193)%
	CASE 21
		LOCALS = rotor
	CASEELSE
		LOCALS = %ITEMNAME_TR(TFLAG:193)%
	ENDSELECT
	;クラウンピースが釣った
	IF TFLAG:194 > 1000
		PRINTFORMW 「Look! I caught something!!」
		PRINTL 
		PRINTFORMDL %CALLNAME:TARGET% caught a %LOCALS%!
		RESETCOLOR
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K111
		SELECTCASE LOCALS
		CASE "ローター"
			PRINTFORMW 「Nn? This is...」
		CASE "カエル"
			PRINTFORMW 「Kyahahaha! A frog! Kero Kero! Kero Kero!」
		CASE "氷の鱗", "龍の爪"
			PRINTFORMW 「What is this!? Beautiful～～!!」
		CASE "龍涎香"
			PRINTFORMW 「なにこれ、ゴミじゃん」
		CASE "ガーパイク", "カムルチー", "シーラカンス", "チョウザメ", "イトウ", "ピラルクー", "タキタロウ"
			PRINTFORMW 「きゃ～！ でっかーい！！ %CALLNAME:MASTER%持つの手伝って～！」
		CASE "ピラニア"
			PRINTFORMW 「うぉ～ピラニアじゃーん！ イカす～！」
		CASE "ウナギ", "ヤツメウナギ", "ナマズ"
			PRINTFORMW 「わお！ ぬるぬるしてるぞ～！」
		CASEELSE
			PRINTFORMW 「How is it!? Amazing, right?」
		ENDSELECT
	;MASTERが釣った
	ELSE
		RESETCOLOR
		PRINTFORMDL You caught a %LOCALS%!
		;ローター以降を弾いてサイズと漁獲数の処理
		IF INRANGE(TFLAG:193, 1, 60) && TFLAG:193 != 21
			;サイズの出力
			LOCAL:1 = FISHING_SIZE(TFLAG:193)
			PRINTFORML 　サイズ：{LOCAL:1}cm
			;ここまでの最高サイズと比較して大きければ記録
			IF LOCAL:1 > FISHSIZE_RECORD:(TFLAG:193)
				SETCOLOR C_YELLOW
				SIF FISH_EXP:(TFLAG:193)
					PRINTFORML 　今まで釣った%LOCALS%の中でも最大サイズだ！
				RESETCOLOR
				FISHSIZE_RECORD:(TFLAG:193) = LOCAL:1
			ENDIF
			;漁獲数の加算
			FISH_EXP:(TFLAG:193) ++
		ENDIF
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K111
		SELECTCASE LOCALS
		CASE "ローター"
			PRINTFORML 「Nn? This is...」
			PRINTFORMW 「%CALLNAME:MASTER%, how lewd you are～!」
		CASE "カエル"
			PRINTFORMW 「Kyahahaha! A frog! Kero kero! Kero kero!」
		CASE "氷の鱗", "龍の爪"
			PRINTFORMW 「What is this!? Beautiful～～!!」
		CASE "龍涎香"
			PRINTFORMW 「なにこれ、ゴミじゃん」
		CASE "ピラニア"
			PRINTFORMW 「うぉ～ピラニアじゃーん！ イカす～！」
		CASE "ウナギ", "ヤツメウナギ", "ナマズ"
			PRINTFORMW 「きゃははは！ ぬるぬる～！」
		CASEELSE
			IF LOCAL:1 >= 80
				PRINTFORMW 「わお！ でっかーい！！」
			ELSEIF INRANGE(TFLAG:193, 11, 20) || INRANGE(TFLAG:193, 24, 60)
				PRINTFORMW 「おー！ すごい！ それ、食べるの？」
			ELSEIF TALENT:111:恋慕 || TALENT:111:思慕
				PRINTFORMW 「Wa! Amazing! What is this??」
			ELSE
				PRINTFORMW 「おー！ やるじゃ～ん！」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
