;==================================================
@M_KOJO_EVENT_K57_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FLAG:時間停止
		ELSEIF CFLAG:318 == 1
			PRINTFORMW 
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%、丁度会いたかったと思ってたところなのよ～」
					PRINTFORMW こころは嬉しそうな笑顔を浮かべて話しかけてきた
				CASE 0
					PRINTFORML 「おぉ、いい所に来たな%CALLNAME:MASTER%」
					PRINTFORMW 「丁度暇をしていたところなの」
				CASEELSE
					PRINTFORML 「おぉ～%CALLNAME:MASTER%、会えてうれしいわ」
					PRINTFORMW 「というわけで喜びの面っと…」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%、会いに来てくれたの？」
					PRINTFORMW 
				CASE 0
					PRINTFORML 「いらっしゃい、会えてうれしいわ」
					PRINTFORMW 「えっと、喜びの面喜びの面…」
				CASEELSE
					PRINTFORMW 「あ、%CALLNAME:MASTER%、どうしたんだ？」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「あれ？ 何か用？」
					PRINTFORMW 
				CASE 0
					PRINTFORMW 「あれれ？ どうしたの？」
				CASEELSE
					PRINTFORMW 「おぉ、奇遇ですねー」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		RETURN 1
	ENDIF
