	IF TALENT:恋慕
		TFLAG:192 = 1
	ELSE
		TFLAG:192 = -1
	ENDIF
ELSE
	CALL TRAIN_MESSAGE
ENDIF
CALL M_KOJO_MESSAGE_COM_K63_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(302)
		PRINTFORMW 「H-Hey! Don't just nonchalantly touch me!」
		PRINTFORMW 「Ah, Sorry... It was so sudden I was surprised......」
		SIF TALENT:思慕
			PRINTFORMW 「Well... If it's %CALLNAME:MASTER%, then just for a bit......」
		RETURN 1
	ENDIF
	
	;恋慕（お風呂）
	IF TALENT:恋慕 && BATHROOM()
		PRINTFORMW 「You'll wash my back?」
		PRINTFORMW 「Thank you......」
		PRINTL
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「Umm... Please don't stare too much......」
		CASE 1
			PRINTFORMW 「Aha♪ I-I'm ticklish there! Fufu.」
		CASE 2
			PRINTFORMW 「Okay, I'm next......」
		ENDSELECT
		RETURN 1
	ENDIF
	IF TCVAR:発情
		FONTBOLD
		CALL HPH_PRINT,@"「ひゃん゛っっHPH」","W"
		FONTREGULAR
		IF TALENT:恋慕
			CFLAG:溜まってる度 += 100
			PALAM:TARGET:潤滑 += 500
			BASE:TARGET:ムード += 500
			PRINTFORMW 「ご、ごめんなさい……今触られるのは困るっていうか……その、嫌ってわけじゃ……」
			PRINTFORMW （うう、%CALLNAME:MASTER%に触られるとカラダが交尾の準備しちゃう……）
			IF TALENT:自己愛 >= 0
				CFLAG:TARGET:同行 = 60
				CFLAG:MASTER:同行 = 60
				PRINTFORMW 「ほら……わかるでしょ？　ね、向こう行きましょ？」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の袖を握りしめた
			ENDIF
		ELSEIF TALENT:思慕
			PRINTFORMW 「ご、ごめんなさい……ちょっと今はそっとしておいて欲しいかなって……」
		ELSE
			PRINTFORMW 「ちょっと触らないでよ！離れて！」
			CFLAG:ブチギレ = 1
		ENDIF
		CALL COLORMESSAGE(@"欲求不満が上がった",C_YELLOW,1)
		CFLAG:溜まってる度 += 100
		IF PALAM:TARGET:潤滑 < 3000
			CALL COLORMESSAGE(@"潤滑が上がった",C_YELLOW,1)
			PALAM:TARGET:潤滑 += 500
		ENDIF
		RETURN 1
	ENDIF
	;恋慕
	IF TALENT:恋慕
		CALL KAGEROU_SKINSHIP
		SELECTCASE RESULT
		;手を握る
		CASE 0
			IF RAND:2
	PRINTFORMW 「Ah... Wai......」
	PRINTFORMW 「My hands...my claws are quite sharp, so be careful...」
			ELSE
	PRINTFORMW 「Fufu, It's faint, but I can feel %CALLNAME:MASTER%'s warmth......」
	PRINTFORMW 「With our fingers entwined like this......」
	PRINTFORMW 「Ah, It's a bit embarrassing, so let's leave it at that......」
			ENDIF
		;頬を撫でる
		CASE 1
			IF RAND:2
	PRINTFORMW 「Ah... What is it, %CALLNAME:MASTER%......」
	PRINTFORMW 「Fufu, I'll do it too......」
			ELSE
	PRINTFORMW 「Wah! You surprised me, all of a sudden.」
	PRINTFORMW 「D-Don't pinch, okay?」
			ENDIF
