;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:Ｃ絶頂～3 CVAB絶頂
;NOWEX:Ｍ絶頂 二重絶頂
;NOWEX:三重絶頂 三重絶頂
;NOWEX:二重絶頂 四重絶頂
;NOWEX:噴乳 噴乳
;NOWEX:射精 射精
;NOWEX:放尿 放尿
;適当に組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K54
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;四重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重絶頂
		PRINTFORMW 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:三重絶頂
		SELECTCASE NOWEX:三重絶頂
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 0
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 0
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 0
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:Ｍ絶頂
		SELECTCASE NOWEX:Ｍ絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 0
			;Ｃ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 0
			;Ｖ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 0
			;Ｃ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 0
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 0
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	IF NOWEX:Ｃ絶頂
		;強絶頂Ｃ
		IF NOWEX:Ｃ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		;強絶頂Ｖ
		IF NOWEX:Ｖ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;強絶頂Ａ
		IF NOWEX:Ａ絶頂 == 2
			PRINTFORMW 
		;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ絶頂
		;強絶頂Ｂ
		IF NOWEX:Ｂ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｂ
		ELSE
			PRINTFORMW 
