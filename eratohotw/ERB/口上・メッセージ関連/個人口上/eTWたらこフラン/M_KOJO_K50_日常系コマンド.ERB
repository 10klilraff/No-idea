@M_KOJO_MESSAGE_COM_K50_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K50_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K50_300_1

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時間停止中は無言 喋らせる際は;を消して「台詞」を加筆の事
	IF FLAG:70 == 1 && CFLAG:TARGET:344 == 0
		;PRINTFORML 「」
		RETURN 1
	ENDIF
	;睡眠中
	IF CFLAG:睡眠 == 1
		RETURN 1
	ENDIF
	;時間停止以外でシラフでない場合
	IF !SHIRAHU(TARGET)
		;PRINTFORML 「」
		RETURN 1
	ENDIF
	;相手主導押し倒され中
	IF CFLAG:うふふ == 2
		;ちんこついてる
		IF TALENT:TARGET:2 & 2
			;PRINTFORML 「」
		;ちんこついてない
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	ENDIF
	;あなた主導押し倒し中
	IF CFLAG:うふふ == 1
		;PRINTFORML 「」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;特殊分岐 使用時にはコメントアウトを解放してください
	;-------------------------------------------------
	;妊娠中
	;IF TALENT:妊娠
	;	PRINTFORML 「」
	;	RETURN 1
	;ENDIF
	;精液ボテ
	;IF !TALENT:妊娠 && EX:TARGET:膣内精液 >= 1000
	;	PRINTFORML 「」
	;	RETURN 1
	;ENDIF
	;マイナスになってもいいさと考えるんだ
	;IF !CFLAG:1502 && CFLAG:信頼度 >= 500 && ABL:教養 > 2 && MONEY >= 300000
	;IF !CFLAG:1502 && CFLAG:信頼度 >= 500 && ABL:教養 > 2
	;	TRYCALLFORM M_KOJO_MESSAGE_COM_K50_進学
	;	RETURN 1
	;ENDIF
	
	IF 家庭菜園可(CFLAG:MASTER:現在位置) && !GETBIT (TCVAR:350,1)
		SETBIT TCVAR:350,1
		;SELECTCASE PLANT_TYPE_0
		SELECTCASE PLANT_TYPE
			CASE 151
				PRINTFORMW 「Ah, cabbage farm.」
				PRINTFORML 「Planting so many things in the garden%UNICODE(0x2661)%」
				PRINTFORMW 「Onii-sama, are you preparing for a baby?」
				IF CFLAG:生解禁 <= 2 && TALENT:恋人
					IF CFLAG:16:信頼度 < 500
						PRINTFORML 「But Onee-sama says we can't.」
					ELSE
						PRINTFORML 
						PRINTFORML 「Okay,」
						PRINTFORMW 「because %CALLNAME:TARGET% also want Onii-sama's baby.」
						PRINTFORMW 「Ufufu! Come on, Onii-sama! Try to catch %CALLNAME:TARGET% in the cabbage field!」
						CFLAG:生解禁 = 2
					ENDIF
				ELSE
					PRINTDATAL
						DATALIST
							DATAFORM 「It's a bit too early for %CALLNAME:TARGET%, maybe.」
							;ばっちゃに語呂を合わせぱっちぇ呼称
							DATAFORM 「Patchie says it's impossible before %CALLNAME:TARGET% has her sekihan(red rice, celebration for a girl's first period).」
						ENDLIST
						DATALIST
							DATAFORM 「Onii-sama, not yet.」
							DATAFORM 「No doing it by force, okay? Ufufu♪」
						ENDLIST
						DATALIST
							DATAFORM 「You want to, don't you? O-ni-i-sa-ma?」
							DATAFORM 「%CALLNAME:TARGET% can see it in your eyes, after all.」
							DATAFORM 「Ufufu, Nooooot yet.%UNICODE(0x2661)%」
						ENDLIST
					ENDDATA
				ENDIF
			CASE 163
				PRINTFORML 「Hmmm, Was Onee-sama green when she was young too?」
			CASE 164
				PRINTFORML 「Watermelon-san～ Watermelon-san～ Grow biiiig and strong, okay? Watermelon-san～」
				PRINTFORML 「Let's eat watermelon with everyone!」
				PRINTFORMDW %CALLNAME:TARGET% laughs.
			CASE 177
				PRINTFORML 「Let's make wine with these grapes.」
				PRINTFORML 「%CALLNAME:TARGET% wants to do the stomping, okay?」
				PRINTFORML 「Ufufu, It's so fun♪」
				PRINTFORMD %CALLNAME:TARGET% holds up her skirt slightly, 
				
				IF TALENT:MASTER:マゾ
					PRINTFORMDL and as she does so, her eyes drift towards your crotch, her tiny feet stomping about.
				ELSE
					PRINTFORMDL stomping about on a pantomime grape treading spree.
				ENDIF
			CASE 187
				PRINTFORML 「Yuri～♪ Yuriyuri～♪」 (Yuri is the Japanese word for lily)
				PRINTFORML 「Onii-sama, did you know, did you know? You can eat the root of the lily!」
				SIF TALENT:MASTER:性別 == 3
					PRINTFORMDL %CALLNAME:TARGET% looks at your crotch somewhat weirdly.
			CASE 188
				IF TCVAR:媚薬
					PRINTFORML 「Onii-sama, did you know did you know,」
					PRINTFORML 「if you squeeze a lemon on a carrot, it turns into an ashprodeeshiak. But what is an ashprodeeshiak anyway?」
				ELSE
					PRINTFORML 「Carrot-san♪ Raddish-san♪ Shiitake-san♪ Lappa-san♪ Lotus root-san who is full of holes♪」
					PRINTFORML 「And bittercur-san who is full of vines♪」
				ENDIF
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;以下日常 とりあえずランダム三枠
	;-------------------------------------------------
	
	IF TFLAG:193 == 1
		PRINTFORMDW %CALLNAME:TARGET% nods her head in agreement as you speak.
		RETURN 1
	ELSEIF TFLAG:193 == -1
		PRINTFORMDW %CALLNAME:TARGET% shakes her head a little at your words.
		RETURN 1
	ENDIF
	;恋慕 後期
	IF TALENT:恋慕
		IF !RAND:3
			;PRINTFORML 「」
		ELSEIF !RAND:2
			;PRINTFORML 「」
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	;思慕 中期
	ELSEIF TALENT:思慕
		IF !RAND:3
			;PRINTFORML 「」
		ELSEIF !RAND:2
			;PRINTFORML 「」
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		IF !RAND:3
			;PRINTFORML 「」
		ELSEIF !RAND:2
			;PRINTFORML 「」
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	ENDIF
	;ここに口上書いて下さい

ENDIF
RETURN 1
