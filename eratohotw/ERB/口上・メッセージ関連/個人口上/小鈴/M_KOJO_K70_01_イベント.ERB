@M_KOJO_UPDATE_K70
#DIM VERSION
;口上独自のUPDATE用
;ENCOUNTER専用に独自分岐フラグを仕込んだ場合等にどうぞ
;UPDATE判定でCFLAG等のキャラ変数を使う場合、キャラ番号の指定が必要になります
VERSION = 120
SIF TALENT:70:恋慕 || TALENT:70:愛欲 || TALENT:70:セフレ >= 2
	CFLAG:70:1006 = 1
CALL M_KOJO_COLOR_K70
IF !TALENT:70:キス未経験 || EXP:70:キス経験
	SIF FIRSTTIME("ファーストキス", 0, 70)
		PRINTFORML ・Kosuzu has experienced %HIS_HER(70)% first kiss
ENDIF
IF EXP:70:精飲経験
	SIF FIRSTTIME("精液の味", 0, 70)
		PRINTFORML ・Kosuzu knows the taste of semen
ENDIF
RESETCOLOR
IF CFLAG:70:1100 == VERSION
	CALL M_KOJO_COLOR_K70
	PRINTFORML Latest version (ver.{CFLAG:70:1100 / 100}.{CFLAG:70:1100 % 100})
	RESETCOLOR
ELSE
	SIF !CFLAG:70:1002
		CFLAG:70:1002 = DAY
	IF !CFLAG:70:1004 && ABL:70:親密
		PRINTFORML Has Kosuzu's self-introduction already been done?
		PRINTL [1] - Yes
		PRINTL [0] - No
		INPUT
		IF RESULT
			CFLAG:70:1004 = 1
			PRINTFORMW The self-introduction has been done.
		ELSE
			PRINTFORMW There will be a self-introduction some time soon.
		ENDIF
	ENDIF
	IF !CFLAG:70:1005 && ABL:70:親密
		PRINTFORML Set it so the Suzunaan has been visited?
		PRINTL [1] - Yes
		PRINTL [0] - No
		INPUT
		IF RESULT
			PRINTFORML How many times has the Suzunaan been visited? (Doesn't mean anything right now)
			PRINTL [5] - Lots
			PRINTL [3] - A bit
			PRINTL [1] - A little
			PRINTL [0] - On second thought, never
			INPUT
			CFLAG:70:1005 = ABL:70:親密 * LIMIT(RESULT, 0, 5)
		ENDIF
		PRINTFORMW Your visiting experience is now {CFLAG:70:1005}.
	ENDIF
	IF TALENT:70:酒耐性 != -1
		PRINTFORML Set [Weak Drinker]?
		PRINTL [1] - Yes
		PRINTL [0] - No
		INPUT
		IF RESULT
			TALENT:70:酒耐性 = -1
			CALL M_KOJO_COLOR_K70
			PRINTFORMW 「Aha♪」
			RESETCOLOR
		ELSE
			PRINTPLAIN [332] The dialog might be a bit different. Please note.
			PRINTW 
		ENDIF
	ENDIF
	IF RELATION:70:80 < 200
		PRINTFORML Fix affinity with characters Kosuzu works with?
		PRINTL [1] - Yes
		PRINTL [0] - No
		INPUT
		IF RESULT
			;めんどくさいので無駄多いけど80番まで回す
			FOR LOCAL, 1, 81
				IF GROUPMATCH(LOCAL, 1, 11, 26, 29, 69, 80)
					PRINTFORM %CALLNAME:LOCAL, 12, LEFT%{RELATION:70:LOCAL, 3}\% → 
					IF LOCAL < 30
						RELATION:70:LOCAL = 120
					ELSEIF LOCAL == 69
						RELATION:70:LOCAL = 150
					ELSEIF LOCAL == 80
						RELATION:70:LOCAL = 200
					ENDIF
					PRINTFORML {RELATION:70:LOCAL, 3}\%
				ENDIF
			NEXT
			PRINTFORMW Now changed to this.
		ELSE
			PRINTFORMW Not changed.
		ENDIF
	ENDIF
	CFLAG:70:1100 = VERSION
	CALL M_KOJO_COLOR_K70
	PRINTFORML UPDATE complete! (ver.{CFLAG:70:1100 / 100}.{CFLAG:70:1100 % 100})
	RESETCOLOR
ENDIF
RETURN 1

