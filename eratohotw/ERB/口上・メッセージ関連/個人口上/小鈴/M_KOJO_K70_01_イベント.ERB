;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K70_2(ARG,ARG:1)
#DIM 分岐
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			$OHAYOU
			分岐 = RAND:7
			PRINTL 
			IF 分岐 == 0 || TALENT:70:機嫌 == -1
				PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning...」
				PRINTL 
				PRINTFORMDL Kosuzu seems a bit irritated...
				PRINTFORMDL Perhaps something happened yesterday...
				PRINTW 
				SELECTCASE TIME
				CASE 240 TO 599
					PRINTFORMW 「... What is it? It's already morning.」
				CASE 600 TO 1079
					PRINTFORMW 「... What is it? It's already morning?」
				CASEELSE
					PRINTFORMW 「... What is it? It's already night...」
				ENDSELECT
				TALENT:70:機嫌 = -1
			ELSEIF 分岐 == 1 || 分岐 == 2 || TALENT:70:機嫌 == 1
;				;昨晩満足,ズレ修正したけどまだ懸念アリ
				PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning♪」
				IF CFLAG:70:1001 == DAY - 1
					PRINTFORML 「Ehehe%UNICODE(0x2764)% Yesterday was awesome♪」
					PRINTFORMW 「Let's do it again next time♪」
					TALENT:70:機嫌 = 1
				ELSE
					PRINTFORMW 「Aha♪ I was enjoying %CALLNAME:MASTER%-san's sleeping face♪」
				ENDIF
			ELSEIF 分岐 == 3 || 分岐 == 4
				PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning!」
				PRINTFORML 「Fufu♪ It looked like you were having a good time sleeping.」
				PRINTFORMW 「What kind of dream did you have?」
			ELSE
				PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning!」
				IF DAY:2 == 1
					PRINTFORML 「It's getting warmer.」
					PRINTFORMW 「But you can't oversleepー」
				ELSEIF DAY:2 == 2
					PRINTFORML 「It's hot todayー...」
					PRINTFORMW 「Do you want me to wipe the sweat?」
				ELSEIF DAY:2 == 3
					PRINTFORML 「It's getting quite cold.」
					PRINTFORMW 「E-Even if I warmed up... It's fine... \/\/\/」
				ELSEIF DAY:2 == 4
					PRINTFORML 「It's getting colder today...」
					PRINTFORMW 「Even so, don't stay in the futon forever.」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;非同棲時と同じ
		GOTO OHAYOU
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			$OHAOHAYOU
			分岐 = RAND:10
			PRINTL 
			PRINTFORML 「Nn... Munya... Haa...」
			IF M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 2
				IF GROUPMATCH(分岐, 1, 2, 3) || TALENT:70:機嫌 == 1
					PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning～♪」
					IF CFLAG:70:1001 == DAY - 1
						PRINTFORML 「Ehehe%UNICODE(0x2764)% Yesterday was awesome♪」
						PRINTFORMW 「Let's do it again next time♪」
						TALENT:70:機嫌 = 1
					ELSE
						PRINTFORMW 「Fufu♪ %CALLNAME:MASTER%-san appeared in my dream...%UNICODE(0x2764)%」
					ENDIF
				ELSEIF 分岐 == 0 || TALENT:70:機嫌 == -1
					PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning...」
					PRINTL 
					PRINTFORMDL Kosuzu seems to be irritated...
					PRINTFORMDL Perhaps something happened yesterday...
					PRINTW 
					PRINTFORMW 「What is it...? Is there something on my face?」
					TALENT:70:機嫌 = -1
				ELSEIF GROUPMATCH(分岐, 4, 5, 6)
					PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning.」
					PRINTFORMW 「Nhehe...♪ It's nice to see %CALLNAME:MASTER%-san's face when I wake up～」
				ELSE
					PRINTFORMW 「Ah, %CALLNAME:MASTER%-san, good morning.」
					IF DAY:2 == 1
						PRINTFORML 「It's getting quite warm.」
						PRINTFORMW 「I want to sleep a little more...」
					ELSEIF DAY:2 == 2
						PRINTFORML 「It's hot today...」
						PRINTFORMW 「%CALLNAME:MASTER%-san, are you sweating?」
					ELSEIF DAY:2 == 3
						PRINTFORML 「It's getting quite cold.」
						PRINTFORMW 「%CALLNAME:MASTER%-san, please warm me up... \/\/\/」
						;＞＜*
					ELSEIF DAY:2 == 4
						PRINTFORML 「It's getting colder today...」
						PRINTFORMW 「Uuu... I don't want to get out of the futon...」
					ENDIF
				ENDIF
			ELSE
				PRINTFORMW 「...Eh? %CALLNAME:MASTER%-san? That place...」
				PRINTFORMW 「Ah, that's right! I... Excuse me... \/\/\/」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			;非同棲時と同じ
			GOTO OHAOHAYOU
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K70_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時になって寝る）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1 && !CFLAG:70:うふふ
		$OYASUMI
		;小鈴の部屋
		IF M_KOJO_K70_SUZUNAAN(CFLAG:MASTER:現在位置) == 2
			PRINTFORMW 「Fuwaa... I'm getting sleepy...」
			PRINTFORMW 「Good night, %CALLNAME:MASTER%-san...」
			;泊まれる
			IF CFLAG:70:現在位置 == OMANEKIBEYA()
				SIF !TCVAR:(CFLAG:MASTER:お招き):連泊禁止 && NEMUKE() < 720
					PRINTFORMW 「... Will you stay next to me until morning...?」
			ELSEIF TALENT:70:恋慕
				PRINTFORMW 「... Will you stay next to me until morning...?」
			ENDIF
		;それ以外
		ELSE
			PRINTFORMW 「Fuwaa... I'm getting sleepy...」
			IF CFLAG:MASTER:初期位置 == CFLAG:MASTER:初期位置
				PRINTFORMW 「I'll go to bed first... Good night, %CALLNAME:MASTER%-san...」
			ELSEIF MAIN_MAP == 2 && M_KOJO_K70_SUZUNAAN(CFLAG:MASTER:現在位置) == 1
				PRINTFORMW 「Good night, %CALLNAME:MASTER%-san...」
			ELSE
				PRINTFORMW 「I'm going home... Good night...」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「Faa... S-Sleepy...」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「Suu... Suu...」
		CASE 2
			PRINTFORMW 「Kuu... Fuu...」
		CASEELSE
			PRINTFORMW 「Faa... Shtop... Shtooop...」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;衰弱中（恋慕付き拠点内キャラがMAIN_MAPで衰弱状態になった場合か、お招き部屋で相手が衰弱した場合に発生
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「Haa... Haa...」
		PRINTFORMW 「I'm sorry, %CALLNAME:MASTER%-san. I'm tired... Guu...」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（部屋まで運ばれる）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「Ahh!! %CALLNAME:MASTER%-san, are you okay!?」
		PRINTFORML 「Um... W-What should I do...」
		PRINTFORMW 「%CALLNAME:MASTER%-san's room...」
		PRINTFORMW 「Nngh! Oh, heavy...」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが自室まで戻ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			PRINTFORMW 「Nn... I was going to read a book... Sleepy...」
			PRINTFORMW 「Good night... Guu...」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出された
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「W-What are you doing!?」
			PRINTFORMW 「Please get out!!」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとのＨを止めさせて寝た
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORMW 「Geez!! Did you think this was someone else's room!?」
			PRINTFORMW 「I have misjudged you!!」
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;非在住TARGETがMAIN_MAPで休憩
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;前のターンから一緒の部屋にいる状態で相手が寝る（お招き部屋含む（
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;よくわからんのでおやすみなさい
		GOTO OYASUMI
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

