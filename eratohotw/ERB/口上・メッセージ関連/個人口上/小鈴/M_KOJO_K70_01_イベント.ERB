		PRINTFORML 「Kyaa%UNICODE(0x2764)%」
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「Ahh%UNICODE(0x2764)% Please forgive me～!」
		CASEELSE
			PRINTFORMW 「Ahh%UNICODE(0x2764)% I'm sorry～!」
		ENDSELECT
		TCVAR:70:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		;ダーク小鈴ちゃん
		PRINTFORML 「Fufufu... %UNICODE(0x2764)% %CALLNAME:MASTER%-san, it's nice, right...%UNICODE(0x2764)%?」
		PRINTFORMW 「Finally, it's time to use the power of the『Succubus's Garden』...%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
ENDIF
