;分岐用のゲストキャラ判定
@M_KOJO_K70_GUEST(ARG)
#DIM CHARA
;ARG	0=会話, 1=お茶
VARSET RESULT
RESULTS = 
;阿求(80)は最優先
IF CFLAG:80:現在位置 == CFLAG:70:現在位置 && !TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(80, ARG)) && M_KOJO_K70_GUEST_ABLE(80)
	RESULTS = %UN_TR_NAME(CALLNAME:80)%
	RESULT:1 = 80
	RETURN M_KOJO_K70_BALANCE(70, 80)
ENDIF
FOR LOCAL, 1, 10
	;霊夢(1)、魔理沙(11)、紫(26)、文(29)、マミゾウ(69)、阿求(80)
	CHARA = TARGET:LOCAL
	IF GROUPMATCH(CHARA, 1, 11, 26, 29, 69, 80) && !TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(CHARA, ARG)) && M_KOJO_K70_GUEST_ABLE(CHARA)
		RESULTS = %UN_TR_NAME(CALLNAME:CHARA)%
		RESULT:1 = CHARA
		;台詞を先に考えてたらよくわからん分岐になってしまった
		;恋慕の小鈴と同行中（デート含む）
		IF CFLAG:70:同行 && TALENT:70:恋慕
			RETURN 4
		;鈴奈庵でゲストと同行中
		ELSEIF CFLAG:CHARA:同行 && M_KOJO_K70_SUZUNAAN(CFLAG:70:現在位置) == 1
			RETURN 3
		;ゲストとデート中
		ELSEIF CFLAG:CHARA:同行 && FLAG:デート相手 == CHARA
			RETURN 2
		;恋慕のゲストと同行中
		ELSEIF CFLAG:CHARA:同行 && TALENT:CHARA:恋慕
			RETURN 2
		;小鈴が恋慕
		ELSEIF TALENT:70:恋慕
			RETURN 4
		ELSE
			RETURN 1
		ENDIF
	ENDIF
NEXT

;ゲスト可否
@M_KOJO_K70_GUEST_ABLE(ARG)
#FUNCTION
;睡眠中
SIF CFLAG:ARG:睡眠
	RETURNF 0
SIF CFLAG:70:睡眠
	RETURNF 0
;添い寝中
SIF CFLAG:ARG:添い寝中
	RETURNF 0
SIF CFLAG:70:添い寝中
	RETURNF 0
;浴場
SIF BATHCHECK(CFLAG:MASTER:現在位置)
	RETURNF 0
RETURNF 1

;ゲストキャラ変換用
@M_KOJO_K70_GUEST_TCVAR(ARG, ARG:1)
#FUNCTION
VARSET LOCAL
SELECTCASE ARG
;霊夢
CASE 1
	LOCAL = 361
;魔理沙
CASE 11
	LOCAL = 362
;紫
CASE 26
	LOCAL = 363
;文
CASE 29
	LOCAL = 364
;マミゾウ
CASE 69
	LOCAL = 365
;阿求
CASE 80
	LOCAL = 366
ENDSELECT
SIF ARG:1
	LOCAL += 10
RETURNF LOCAL

;関係性比較関数,小鈴と阿求にしか使ってないけど一応汎用できるように
@M_KOJO_K70_BALANCE(ARG:1, ARG:2)
#FUNCTION
;ARG:1	主対象
;ARG:2	比較対象
;独占
IF TALENT:(ARG:1):恋人
	RETURNF 8
;悲恋
ELSEIF TALENT:(ARG:2):恋人 && TALENT:(ARG:1):恋慕
	RETURNF 7
;揶揄
ELSEIF TALENT:(ARG:2):恋人
	RETURNF 6
;信頼
ELSEIF TALENT:(ARG:1):恋慕 && (TALENT:(ARG:2):恋慕 || TALENT:(ARG:2):思慕) && CFLAG:(ARG:1):信頼度 > 1000
	RETURNF 5
;敵対
ELSEIF TALENT:(ARG:1):恋慕 && (TALENT:(ARG:2):恋慕 || TALENT:(ARG:2):思慕)
	RETURNF 4
;恋慕
ELSEIF TALENT:(ARG:1):恋慕
	RETURNF 3
;応援
ELSEIF TALENT:(ARG:2):恋慕
	RETURNF 2
;平穏
ELSE
	RETURNF 1
ENDIF

