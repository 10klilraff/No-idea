	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;時姦中
		IF FLAG:70
		;処女
		ELSEIF TCVAR:破瓜 == 1
			IF TALENT:恋慕
				PRINTFORMW 「私の羽根、ジャマじゃないですか？」
				PRINTFORMDW おずおずと羽根を世話しなく動かし、%CALLNAME:MASTER%にかからないよう動かす大妖精の仕草がいじらしい。
				PRINTFORMW 「えと…こうやって…んんっ」
				PRINTFORMDW 柔らかな桃尻を動かせば程よく%TINKO(PLAYER)%を擦りビクビクと快感が走る。
				PRINTFORMW 「ん…これで入る…かも…んっ―…」
				PRINTFORMDW %TINKO(PLAYER)%をワレメにあてがい、ゆっくりと未通の穴へと誘う。
				PRINTFORMW 「んっ…くぅ…あぅ…も、もう少しで…ぜんぶ･･」
				PRINTFORMDW 破瓜と擦りあげられる痛みに苦悶の表情でしかめるも、じょじょに%TINKO(PLAYER)%が呑み込まれていく。
				PRINTFORMW 「やっ…ん…は、入りました…。――あう…」
				PRINTFORMDW 男根を全て呑み込み、大妖精は身体を力なく%CALLNAME:MASTER%へともたれ掛った。
				PRINTFORMW 「フ、フフ…おっきいです…。こんなのが私の中に入ってるなんて…」
			ELSE
				PRINTFORMDW 大妖精を抱えるように座り乱暴に股を広げさせる。濡れたワレメが露になり露が一滴垂れる。
				PRINTFORMW 「や、やだ…恥ずかしいです…」
				PRINTFORMDW 困惑する大妖精が%CALLNAME:MASTER%を上目遣いで見るも、気にせず太ももを持ち上げ自らの男根へ誘導する。
				PRINTFORMW 「あぅ…はっ…やっ…！ま、まってくださっ…！」
				PRINTFORMDW 大妖精の懇願する言葉をさえぎり、%CALLNAME:MASTER%はワレメを%TINKO(PLAYER)%で串刺しにする。
				PRINTFORMDW 引き攣る大妖精の身体、繋がった男根からツゥと赤い血が滴る。
				PRINTFORMW 「く、苦し…お願…まだ動かなで…ヒャウゥ！」
				PRINTFORMDW お腹を押さえ痙攣する大妖精を意に介さず、%CALLNAME:MASTER%はさらに突き進める為に腰を上下に動かし始めた。
			ENDIF
		ELSE
			PRINTFORMW 「私が挿れますね…んしょ…」
			PRINTFORMDW 大妖精は%CALLNAME:MASTER%に背中を預けると、%TINKO(PLAYER)%にワレメを沈みこませた。
		ENDIF
	ENDIF
	;-------------------------------------------------
	;挿入継続
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;時姦中
		IF FLAG:70
			PRINTFORMW 
		;前回射精した
		ELSEIF TCVAR:102
			IF RAND:3 == 0
				PRINTFORMW 「あっ…はぁ…精子…いっぱい%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「ひゃあ！お腹の中で跳ねてるっ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「もっと、もっと出してくださいっ%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「んんん！んっ、あっ、はぁはぁ…%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「やぁぁ…、そこっ、だめ%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「あっ、やっ、はげしいっ…%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「あっ…もっと奥にきちゃう…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0 
			PRINTFORMW 「ああ、あっ、やっ、やああっ%UNICODE(0x2665) *1%」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「んはぁ、ああう、やあ%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 「ビクッてなるところ…こすれちゃってます…%UNICODE(0x2665) *1%」
	;それ以外
	ELSE
		PRINTFORMW 「ふっくぅ…うん%UNICODE(0x2665) *1%」
	ENDIF
ENDIF
RETURN 1

