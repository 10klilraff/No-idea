;==================================================
@M_KOJO_EVENT_K4_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GROUPMATCH(ARG, 1, 6) ;edit
		PRINTFORML
		IF CFLAG:318 == 1
			PRINTFORML 「What do you want?」
			PRINTFORMDW Mima frowns.
		ELSEIF TALENT:恋人
			PRINTFORML 「Ah, if it isn't %CALLNAME:MASTER%.」
			PRINTFORMDW Mima happily smiles.
		ELSEIF TALENT:恋慕
			PRINTFORML 「Hey, %CALLNAME:MASTER%.」
			PRINTFORMDW Mima greets you warmly.
		ELSEIF TALENT:思慕
			PRINTFORML 「Oh, it's you.」
			PRINTFORMDW When Mima notices you she briefly greets you.
		ELSE
			PRINTFORMDW Mima glances at you and then directs her sight somewhere else.
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		IF CFLAG:318 == 1
			PRINTFORML 「...」
			PRINTFORMDW Mima completely ignores you.
		ELSEIF TALENT:恋人
			PRINTFORML 「Ah, if it isn't %CALLNAME:MASTER%.」
			PRINTFORMDW Mima happily smiles.
		ELSEIF TALENT:恋慕
			PRINTFORML 「Hey, %CALLNAME:MASTER%.」
			PRINTFORMDW Mima greets you warmly.
		ELSEIF TALENT:思慕
			PRINTFORML 「Oh, it's you.」
			PRINTFORMDW When Mima notices you she briefly greets you.
		ELSE
			PRINTFORMDW Mima glances at you and then directs her sight somewhere else.
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML
		IF CFLAG:318 == 1
			PRINTFORML 「...」
			PRINTFORMDW Mima completely ignores you.
		ELSEIF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%, mind if I join in?」
			PRINTFORMDW Mima sits next to you, exposing her voluptuous body.
		ELSEIF TALENT:恋慕
			PRINTFORML 「I'm coming in.」
			PRINTFORMDW Mima steps into the bath as she tries to cover her voluptuous body with a small towel.
		ELSEIF TALENT:思慕
			PRINTFORML 「Make some space.」
			PRINTFORMDW Mima steps into the bath with you, making you feel a little worried.
		ELSE
			PRINTFORML 「Make some space.」
			PRINTFORMDW Mima comes into the bath, ignoring your presence.
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		IF CFLAG:318 == 1
			PRINTFORML 「...」
			PRINTFORMDW Mima silently leaves as soon as she sees you.
		ELSEIF TALENT:恋人
			PRINTFORML 「Oh sorry, did you enter here first?」
			PRINTFORMDW Mima exits the bath after confirming your presence.
		ELSEIF TALENT:恋慕
			PRINTFORML 「Oh my...I'm sorry.」
			PRINTFORMDW Mima leaves the bath blushing after she sees you naked.
		ELSEIF TALENT:思慕
			PRINTFORML 「Ah, I didn't know you were bathing.」
			PRINTFORMDW Mima hurriedly leaves the bath after confirming your presence.
		ELSE
			PRINTFORML 「Oh, you were here.」
			PRINTFORMDW After Mima sees you, she sighs and leaves.
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

